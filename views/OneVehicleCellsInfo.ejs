<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>甲醇重整燃料电池系统</title>
    <link rel="shortcut icon" href="../../img/favicon.ico" />
    <link rel="stylesheet" href="../css/bootstrap.min.css">
    <link rel="stylesheet" href="../css/offcanvas.min.css">
    <link rel="stylesheet" href="../css/default.css">
</head>

<body>
    <div class="overlay"></div>
    <nav class="navbar navbar-inverse navbar-static-top">
        <div class="">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed pull-left" data-toggle="offcanvas">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">甲醇重整燃料电池系统</a>
            </div>
            <div id="navbar" class="collapse navbar-collapse sidebar-offcanvas">
                <ul class="nav navbar-nav">
                    <li role="presentation" class="navli">
                        &nbsp;&nbsp;<span style="height:50px;line-height:50px;color:#fff;">车辆：</span><span style="height:50px;line-height:50px;color:#fff;"
                            id="vname" name="vname">
                            <%= items[0].vname%></span>
                    </li>
                    <% items.forEach(function(item) {%>
                    <li role="presentation" class="navli">
                        <a href="../../devicesInfo4/<%= items[0].vname%>/<%=item.guid %>">
                            <%=item.guid %></a>
                    </li>
                    <%});%>
                    <li role="presentation" class="active navli">
                        <a href="../../OneVehicleCellsInfo/<%= items[0].vname%>">all</a>
                    </li>
                    <li role="presentation" class="navli">
                        <a id="goback" href="/vehicleList" style="color:#fff;">选择车辆</a>
                    </li>
                    <li role="presentation" class="navli">
                        <a id="logout" href="/logout">退出</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <div class="top text-center">
                <table id="datatable_example" class="responsive table table-striped table-bordered text-center" style="width:100%;margin:10px;">
                    <thead>
                        <tr>
                            <th class="no_sort text-center">序号</th>
                            <th class="no_sort text-center">guid</th>
                            <th class="no_sort text-center">别名</th>
                            <th class="no_sort text-center">所属容器</th>
                            <th class="no_sort text-center">控制人员</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for(var i=0;i<items.length;i++){ %>
                        <tr>
                            <td class="no_sort">
                                <%=i+1 %>
                            </td>
                            <td class="no_sort">
                                <%=items[i].guid %>
                            </td>
                            <td class="no_sort to_hide_phone">
                                <%=items[i].alias %>
                            </td>
                            <td class="no_sort to_hide_phone">
                                <%=items[i].vid %>
                            </td>
                            <td class="no_sort to_hide_phone">
                                <%=items[i].uid %>
                            </td>
                        </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <script src="../js/jquery.min.js"></script>
    <script src="../js/bootstrap.min.js"></script>
    <script src="../js/offcanvas.min.js"></script>
</body>

</html>